{
  "name": "@ionic/pwa-elements",
  "version": "0.0.14",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/es5/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@ionic/core": "4.0.0-beta.12",
    "@stencil/core": "0.13.2",
    "@stencil/sass": "0.0.3"
  },
  "devDependencies": {
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1",
    "workbox-build": "3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/ionic-pwa-elements.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/ionic-pwa-elements"
  },
  "homepage": "https://github.com/ionic-team/ionic-pwa-elements",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
